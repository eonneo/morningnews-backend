
{
    "version": 2,
    "builds": [{
        "src": "app.js",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "app.js"
    }],
    "headers": [{
        "src": "/(.*)",
        "headers": [{
            "key": "access-control-allow-origin",
            "value": "*"
        }]
    }]
}